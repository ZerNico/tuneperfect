{
  "name": "tuneperfect",
  "scripts": {
    "dev": "turbo run dev --parallel --force",
    "build": "turbo run build",
    "tuneperfect": "bun run ./node_modules/@tuneperfect/cli/src/index.ts"
  },
  "devDependencies": {
    "turbo": "^2.7.2"
  },
  "dependencies": {
    "@biomejs/biome": "2.0.6",
    "@tuneperfect/cli": "workspace:*"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "packageManager": "bun@1.3.5"
}
